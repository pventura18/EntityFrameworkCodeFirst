<Window x:Class="EntityFrameworkCodeFirst.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EntityFrameworkCodeFirst"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Content="Boto 1" Grid.Column="0" Grid.Row="0" Margin="10" Name="btnCustomers" Click="btnCustomers_Click"/>
        <Button Content="Boto 2" Grid.Column="1" Grid.Row="0" Margin="10"/>
        <Button Content="Boto 3" Grid.Column="0" Grid.Row="1" Margin="10"/>
        <Button Content="Boto 4" Grid.Column="1" Grid.Row="1" Margin="10"/>
        <Button Content="Boto 5" Grid.Column="0" Grid.Row="2" Margin="10"/>
        <Button Content="Boto 6" Grid.Column="1" Grid.Row="2" Margin="10"/>
        <Button Content="Boto 7" Grid.Column="0" Grid.Row="3" Margin="10"/>
        <Button Content="Boto 8" Grid.Column="1" Grid.Row="3" Margin="10"/>
        <Button Content="Boto 9" Grid.Column="0" Grid.Row="4" Margin="10"/>
        <Button Content="Boto 10" Grid.Column="1" Grid.Row="4" Margin="10"/>
        <Button Content="Boto 11" Grid.Column="0" Grid.Row="5" Margin="10"/>
        <Button Content="Boto 12" Grid.Column="1" Grid.Row="5" Margin="10"/>
    </Grid>
</Window>
